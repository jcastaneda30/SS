datos <- read.table("TiemposDia1Sec.txt")
datos <- as.numeric(datos$V1)
mean(datos)
library("MASS")
library("car")
library("fitdistrplus")
library("moments")
plotdist(datos,histo=TRUE,demp=TRUE)
var(datos)
summary(datos)
descdist(datos,boot = 1000)

fit_exponential <- fitdist(datos, "exp")
fit_lognormal <- fitdist(datos, "lnorm")

plot(fit_exponential)
summary(fit_exponential)

plot(fit_lognormal)
summary(fit_lognormal)

gofstat(list(fit_exponential,fit_lognormal))












# Definir el nivel de confianza
nivel_confianza <- 0.975

# Obtener la desviaci칩n est치ndar de los datos en la columna V1
desviacion_estandar <- sd(datos$V1)

# Calcular el tama침o de muestra
tamanio_muestra <- (qnorm(nivel_confianza) * desviacion_estandar / 4)^2

# Mostrar el tama침o de muestra calculado
print(tamanio_muestra)
